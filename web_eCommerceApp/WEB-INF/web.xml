<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ServletControl</servlet-name>
        <servlet-class>servlets.ServletControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletContest</servlet-name>
        <servlet-class>servlets.ServletContest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletControl</servlet-name>
        <url-pattern>/ServletControl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletContest</servlet-name>
        <url-pattern>/ServletContest</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            1
        </session-timeout>
    </session-config>
    <listener>
        <listener-class>servlets.ServletControl</listener-class>
    </listener>
</web-app>
